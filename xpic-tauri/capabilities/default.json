{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "core:window:allow-center",
    "core:window:allow-set-size",
    "core:window:allow-set-min-size",
    "dialog:default",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-copy-file"
  ]
}
